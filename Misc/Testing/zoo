ADD ANIMAL

$an = new animal();
$data = array(
	'name'=>'Blackbuck'
	,'size_height'=> 50
	,'size_width'=>75
	,'size_length'=>60
	,'location_latitude'=>1.40495
	,'location_longitude'=>103.79049
	,'doors'=>2
	,'animal_limit'=>2
		
	);
$an->add($data);

